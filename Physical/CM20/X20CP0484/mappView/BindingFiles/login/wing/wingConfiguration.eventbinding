<?xml version="1.0" encoding="utf-8"?>
<EventBindingSet id="wingConfiguration_eventbinding" xmlns="http://www.br-automation.com/iat2014/eventbinding/v2" xmlns:types="http://www.br-automation.com/iat2015/widgetTypes/v2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Bindings>
  	<!-- recipe PAGE BUTTON -->
	<EventBinding id="wingConfiguration.recipe.MouseDown">
		<Source contentRefId="wingConfiguration" widgetRefId="recipe" xsi:type="widgets.brease.Button.Event" event="MouseDown" />
		<EventHandler>
			<Action>
				<Target xsi:type="clientSystem.Action">
					<Method xsi:type="clientSystem.Action.Navigate" pageId="wingRecipe" />
				</Target>
			</Action>
		</EventHandler>
	</EventBinding>
	<!-- readout PAGE BUTTON -->
	<EventBinding id="wingConfiguration.readout.MouseDown">
		<Source contentRefId="wingConfiguration" widgetRefId="readout" xsi:type="widgets.brease.Button.Event" event="MouseDown" />
		<EventHandler>
			<Action>
				<Target xsi:type="clientSystem.Action">
					<Method xsi:type="clientSystem.Action.Navigate" pageId="wingReadout" />
				</Target>
			</Action>
		</EventHandler>
	</EventBinding>
  	<!-- IO PAGE BUTTON -->
	<EventBinding id="wingConfiguration.io.MouseDown">
		<Source contentRefId="wingConfiguration" widgetRefId="io" xsi:type="widgets.brease.Button.Event" event="MouseDown" />
		<EventHandler>
			<Action>
				<Target xsi:type="clientSystem.Action">
					<Method xsi:type="clientSystem.Action.Navigate" pageId="wingIO" />
				</Target>
			</Action>
		</EventHandler>
	</EventBinding>
  </Bindings>
</EventBindingSet>
