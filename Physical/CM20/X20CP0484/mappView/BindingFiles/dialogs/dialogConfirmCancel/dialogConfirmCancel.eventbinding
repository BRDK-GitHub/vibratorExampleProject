  <Bindings>
    <EventBinding id="dialogConfirmCancel.cancelButton.Click">
      <Source xsi:type="widgets.brease.Button.Event" contentRefId="dialogConfirmCancel" widgetRefId="cancelButton" event="Click" />
      <Operand datatype="ANY_INT" name="state">
        <ReadTarget xsi:type="session.VariableAction.Read" refId="dialog.confirmCancelState">
          <Method xsi:type="session.VariableAction.GetValue" />
        </ReadTarget>
      </Operand>
      <Operand datatype="ANY_INT" name="quantity">
