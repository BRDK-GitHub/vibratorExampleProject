          <Method xsi:type="opcUa.NodeAction.GetValue" />
        </ReadTarget>
      </Operand>
      <EventHandler condition="state = 70">
        <Action>
          <Target xsi:type="opcUa.NodeAction" refId="::updateCM:this.hmi.automatic.alreadyExists">
            <Method xsi:type="opcUa.NodeAction.SetValueBool" value="false" />
          </Target>
        </Action>
      </EventHandler>
      <EventHandler condition="state = 80">
        <Action>
          <Target xsi:type="session.VariableAction" refId="main.itemQuantity">
            <Method xsi:type="session.VariableAction.SetValueNumber" value="=quantity" />
          </Target>
        </Action>
      </EventHandler>
      <EventHandler>
        <Action>
          <Target xsi:type="clientSystem.Action">
            <Method xsi:type="clientSystem.Action.CloseDialog" dialogId="dialogConfirmCancel" />
          </Target>
        </Action>
      </EventHandler>
    </EventBinding>
